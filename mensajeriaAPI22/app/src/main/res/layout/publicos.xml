<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="415dp"
        android:layout_height="89dp"
        android:layout_marginTop="4dp"
        android:background="#A5D0A7"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9">

        <LinearLayout
            android:layout_width="168dp"
            android:layout_height="106dp"
            android:orientation="horizontal"></LinearLayout>

        <Button
            android:id="@+id/button11"
            android:layout_width="193dp"
            android:layout_height="110dp"
            android:layout_weight="1"
            android:backgroundTint="#A5D0A7"
            android:fontFamily="@font/luckiest_guy"
            android:text="privados"
            android:textSize="20sp" />

        <LinearLayout
            android:layout_width="82dp"
            android:layout_height="91dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:orientation="vertical" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="72dp"
                android:layout_height="61dp"
                app:srcCompat="@drawable/configuracion2" />
        </LinearLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="190dp"
        android:layout_height="185dp"
        android:layout_marginTop="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.013"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_direct_message" />

    <Button
        android:id="@+id/button12"
        android:layout_width="145dp"
        android:layout_height="120dp"
        android:layout_marginTop="168dp"
        android:layout_marginEnd="248dp"
        android:backgroundTint="#56AC4F"
        android:fontFamily="@font/luckiest_guy"
        android:text="publicos"
        android:textSize="20sp"
        app:cornerRadius="@dimen/cardview_compat_inset_shadow"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="56dp"
        android:fontFamily="@font/luckiest_guy"
        android:text="direct message"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="416dp"
        android:layout_height="496dp"
        android:background="#A49696"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:background="#A49696"
            android:orientation="vertical">

        </LinearLayout>

        <Button
            android:id="@+id/button18"
            android:layout_width="match_parent"
            android:layout_height="113dp"
            android:backgroundTint="#090909"
            android:text="Button"
            app:cornerRadius="@dimen/cardview_compat_inset_shadow" />
    </LinearLayout>

    <Button
        android:id="@+id/button19"
        android:layout_width="148dp"
        android:layout_height="58dp"
        android:text="Button"
        tools:layout_editor_absoluteX="283dp"
        tools:layout_editor_absoluteY="800dp" />

</androidx.constraintlayout.widget.ConstraintLayout>